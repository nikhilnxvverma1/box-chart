{
  "name": "box-chart",
  "version": "0.1.0",
  "description": "A semantics aware software diagramming tool that is both minimal and intuitive",
  "scripts": {
    "start": "gulp",
    "start:server": "gulp start:server",
    "start:client": "gulp start:client",
	"compile-sources": "gulp server-typescript",
    "webpack-dev": "webpack --watch --config config/webpack.dev.js",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
